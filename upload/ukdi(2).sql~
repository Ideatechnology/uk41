(2, '2015-02-21 09:19:02', '[{"id_soal":"433","answer":"5","status":"Salah"},{"id_soal":"434","answer":"1","status":"Salah"},{"id_soal":"435","answer":"2","status":"Salah"},{"id_soal":"436","answer":"3","status":"Salah"},{"id_soal":"438","answer":"5","status":"Salah"}]', 'tidak', 1, 10, 0, 5, 0, 0),
(3, '2015-02-21 09:25:48', '[{"id_soal":"433","answer":"5","status":"Salah"},{"id_soal":"434","answer":"6","status":"Tidak dijawab"},{"id_soal":"435","answer":"2","status":"Salah"},{"id_soal":"436","answer":"3","status":"Salah"},{"id_soal":"437","answer":"1","status":"Salah"},{"id_soal":"438","answer":"3","status":"Salah"}]', 'tidak', 6, 10, 0, 5, 1, 0);

-- --------------------------------------------------------

--
-- Table structure for table `bf_kota`
--

CREATE TABLE IF NOT EXISTS `bf_kota` (
`id_kota` int(11) NOT NULL,
  `id_provinsi` varchar(2) DEFAULT NULL,
  `kode` varchar(5) DEFAULT NULL,
  `nama_kota` varchar(50) DEFAULT NULL,
  `ongkos_kirim` decimal(10,0) DEFAULT '0',
  `ongkos_kirim_tiki_yes` decimal(10,0) DEFAULT '0',
  `ongkos_kirim_jne` decimal(10,0) DEFAULT '0',
  `ongkos_kirim_jne_yes` decimal(10,0) DEFAULT '0',
  `ongkos_kirim_pos_kilat` decimal(10,0) DEFAULT '0',
  `ongkos_kirim_pos_biasa` decimal(10,0) DEFAULT '0'
) ENGINE=MyISAM AUTO_INCREMENT=571 DEFAULT CHARSET=latin1;

