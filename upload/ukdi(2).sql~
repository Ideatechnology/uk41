--
-- Table structure for table `bf_post_meta`
--

CREATE TABLE IF NOT EXISTS `bf_post_meta` (
`id` int(10) NOT NULL,
  `judul` varchar(255) CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT NULL,
  `slug_judul` text CHARACTER SET latin1 COLLATE latin1_general_ci,
  `isi` longtext COLLATE utf8_unicode_ci,
  `post_category` int(10) DEFAULT '0',
  `image_data` varchar(255) CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT '',
  `file_data` varchar(255) CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT NULL,
  `author` varchar(50) CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT 'Admin',
  `status_tampil` int(11) DEFAULT '0' COMMENT '0:tampil , 1:draft',
  `flag_comments` int(11) DEFAULT '0' COMMENT '1=tampil;0=tidak tampil',
  `image_data_desc` varchar(255) CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT NULL,
  `modified_on` datetime DEFAULT NULL,
  `created_on` datetime DEFAULT NULL,
  `deleted` int(11) DEFAULT '0' COMMENT '0:tampil;1:soft elete',
  `baca` int(11) DEFAULT NULL,
  `set_img` enum('thumb','none','full') CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT 'thumb',
  `slug_judul_english` text CHARACTER SET latin1 COLLATE latin1_general_ci,
  `isi_english` longtext COLLATE utf8_unicode_ci,
  `judul_english` text CHARACTER SET latin1 COLLATE latin1_general_ci,
  `headline` enum('N','Y') CHARACTER SET latin1 COLLATE latin1_general_ci DEFAULT 'N',
  `meta_description` text CHARACTER SET latin1 COLLATE latin1_general_ci,
  `meta_keyword` text CHARACTER SET latin1 COLLATE latin1_general_ci,
  `meta_robots` text CHARACTER SET latin1 COLLATE latin1_general_ci
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

