--
-- Table structure for table `bf_soal`
--

CREATE TABLE IF NOT EXISTS `bf_soal` (
`id` int(11) NOT NULL,
  `question_name` text COLLATE utf8_unicode_ci NOT NULL,
  `answer1` varchar(250) CHARACTER SET latin1 NOT NULL,
  `answer2` varchar(250) CHARACTER SET latin1 NOT NULL,
  `answer3` varchar(250) CHARACTER SET latin1 NOT NULL,
  `answer4` varchar(250) CHARACTER SET latin1 NOT NULL,
  `answer5` text CHARACTER SET latin1 NOT NULL,
  `answer` varchar(250) CHARACTER SET latin1 NOT NULL,
  `ujian_id` int(11) NOT NULL,
  `urutan` int(11) NOT NULL
) ENGINE=InnoDB AUTO_INCREMENT=439 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;

