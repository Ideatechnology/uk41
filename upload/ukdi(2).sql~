-- --------------------------------------------------------

--
-- Table structure for table `bf_ujian`
--

CREATE TABLE IF NOT EXISTS `bf_ujian` (
`kode_ujian` int(11) NOT NULL,
  `judul_ujian` varchar(200) DEFAULT NULL,
  `keterangan` text,
  `nilai_kelulusan` int(11) DEFAULT NULL COMMENT 'dalam kondisi persen dengan nilai mak 100%',
  `durasi_waktu` int(11) DEFAULT NULL COMMENT 'dalam satuan menit',
  `jumlah_soal` int(11) DEFAULT NULL,
  `is_active` int(11) DEFAULT '0' COMMENT '0=belum aktif;1=aktif',
  `pembayaran` int(11) DEFAULT NULL,
  `modified_on` datetime DEFAULT NULL,
  `created_on` datetime DEFAULT NULL,
  `created_user` int(11) DEFAULT NULL
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=latin1;

--
-- Dumping data for table `bf_ujian`
--

INSERT INTO `bf_ujian` (`kode_ujian`, `judul_ujian`, `keterangan`, `nilai_kelulusan`, `durasi_waktu`, `jumlah_soal`, `is_active`, `pembayaran`, `modified_on`, `created_on`, `created_user`) VALUES
(8, 'Tryout 1', 'lorem gipsum lorem gipsum lorem gipsum lorem gipsum lorem gipsum lorem gipsum lorem gipsum', 70, 120, 200, 1, 0, '2015-02-21 08:16:37', '2015-02-15 08:39:00', 1),
(9, 'tryout 2', 'Lorem Ipsum Lorem IpsumLorem IpsumLorem IpsumLorem IpsumLorem IpsumLorem Ipsum', 70, 120, 200, 1, 500000, NULL, '2015-02-15 08:44:43', 1),
