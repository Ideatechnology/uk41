--
-- Table structure for table `bf_sosmed`
--

CREATE TABLE IF NOT EXISTS `bf_sosmed` (
`id` int(11) NOT NULL,
  `username_twitter` varchar(30) DEFAULT NULL,
  `name_twitter` varchar(40) DEFAULT NULL,
  `password_twitter` varchar(40) DEFAULT NULL,
  `url_twitter` varchar(300) DEFAULT NULL,
  `username_fb` varchar(255) DEFAULT NULL,
  `name_fb` varchar(255) DEFAULT NULL,
  `password_fb` varchar(255) DEFAULT NULL,
  `url_fb` varchar(255) DEFAULT NULL,
  `nama_perusahaan` text,
  `alamat` text,
  `telp` varchar(12) DEFAULT NULL,
  `pin_bb` varchar(13) DEFAULT NULL,
  `whatapps` varchar(255) DEFAULT NULL,
  `logo` varchar(200) DEFAULT NULL,
  `fax` varchar(14) DEFAULT NULL,
  `google_map` text,
  `token_twitter` text,
  `token_fb` text,
  `bank_mandiri` text,
  `bank_bca` text,
  `bank_bni` text,
  `bank_bri` text,
  `paypal` text,
  `email` varchar(100) DEFAULT NULL,
  `an_bank_mandiri` varchar(255) DEFAULT NULL,
  `an_bank_bca` varchar(255) DEFAULT NULL,
  `an_bank_bni` varchar(255) DEFAULT NULL,
  `an_bank_bri` varchar(255) DEFAULT NULL,
  `meta_description` text,
  `meta_robots` text,
  `meta_keyword` text,
  `google_plus_url` text,
  `thumblr_url` text,
  `pinterest_url` text,
  `instagram_url` text,
  `linkedin_url` text,
  `hari_kerja` text,
  `favicon` text,
  `header` text,
  `tentang_perusahaan` text
) ENGINE=InnoDB AUTO_INCREMENT=2 DEFAULT CHARSET=latin1;

